                    </div>
                </div>
            </div>
        </div>
        
        <!-- end col-6 -->
        <!-- begin col-6 -->

        <!-- end col-6 -->
    </div>
    <a href="#modal-dialog" data-toggle="modal" class="btn btn-success btn-icon btn-circle" id="btnShow"  style="width: 40Px;height: 40px; margin-left: 20px;"><i class="fa fa-plus fa-1x" style="margin-top: 12px;" ></i></a>
    <!-- end row -->
</fieldset>
