                    <script>
                      $(document).ready(function() {
                            App.init();
                                $( ".datepicker").datepicker();
    //                                TableManageScroller.init();
                          
        //                   var table = $('#data-table').DataTable();
  var table=   $('#data-table').DataTable({
            dom: 'lBfrtip',
            buttons: [
                { extend: 'copy', className: 'btn-sm' },
                { extend: 'csv', className: 'btn-sm' },
                { extend: 'excel', className: 'btn-sm' },
                { extend: 'pdf', className: 'btn-sm' },
                { extend: 'print', className: 'btn-sm' }
            ],
      responsive: true,
 'rowCallback': function(row, data, index){
         <% if (user.syno != '1'){ %>
               $(row).addClass('hover');
            <% } %>
     
    if(data[7] ==  "Hot"){
              $(row).find('td').css('background-color', '#ffcccc').css('font-weight', 'bold');
            }
            else if(data[7] == 'Warm'){
              $(row).find('td').css('background-color', '#ffd1b3').css('font-weight', 'bold');
            }
	else if(data[7] == 'Cold'){
              $(row).find('td').css('background-color', '#e6f0ff').css('font-weight', 'bold');
            }
		else{
              $(row).find('td').css('font-weight', 'bold');
            }


  }
        });
          

            $('#data-table tbody').on('click', 'tr', function () {
                <% if(user.syno != '1'){%>
                        var data = table.row(this).id();
                          document.getElementById('lval').value=data;
                          var form = $('#lform');
                          form.attr('action', '/historydata');
                          form.attr('method', 'POST');
                          form.submit();
                <%}%>
                    });
                          
                     });                      
               </script>     
